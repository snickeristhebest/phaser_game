{
    "plugins": [
        "plugins/markdown",
        "plugins/summarize"
    ],
    "source": {
        "include": ["box2d-js"],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "opts": {
        "destination": "docs/jsdocs",
        "recurse": true
    },
    "templates": {
        "default": {
            "outputSourceFiles": false
        }
    }    
}
